<tr data-status="{{status}}" data-marker="{{{markerfilter}}}">
        {{! User. }}
        <td>
            <div class="d-flex align-items-center">
                {{#studentimg}}
                    <div class="mr-2 bg-cover bg-light rounded-circle flex-shrink-0" style="background-image: url('{{studentimg}}'); height: 2rem; width: 2rem;"></div>
                {{/studentimg}}
            {{{student}}}
            </div>
        </td>

        {{! Submission. }}
        <td>
            {{#submission}}
                <div class="extensiongranted-container {{^extensiondeadline}}d-none{{/extensiondeadline}}">
                    <span class="badge badge-success">{{#str}} extension_granted, mod_coursework {{/str}}</span><br>
                        <span class="sr-only">{{#str}} due_date, mod_coursework {{/str}}</span>
                        <small class="extensiongranteddate">Due:
                            {{#extensiondeadline}}
                                {{#userdate}}
                                    {{extensiondeadline}},{{#str}}strftimedatetime, langconfig{{/str}}
                                {{/userdate}}
                            {{/extensiondeadline}}
                        </small><br>
                </div>
                {{#submissiondata}}
                    {{#flaggedplagiarism}}
                        <span class="badge badge-danger">{{#str}} flagged_plagiarism, mod_coursework {{/str}}</span><br>
                    {{/flaggedplagiarism}}
                    {{#submittedlate}}
                        <span class="badge badge-warning">{{#str}} submitted_late, mod_coursework {{/str}}</span><br>
                    {{/submittedlate}}
                    {{#datemodified}}
                    <span class="sr-only">{{#str}} date_submitted, mod_coursework {{/str}}</span>
                        <small>
                            {{#userdate}}
                                {{.}},{{#str}}strftimedatetime, langconfig{{/str}}
                            {{/userdate}}
                        </small><br>
                    {{/datemodified}}
                    {{#files}}
                        <span class="sr-only">{{#str}} file_submitted, mod_coursework {{/str}}</span><a href="{{{url}}}"><i class="fa-regular fa-file-lines mr-2"></i>{{filename}}</a><br>
                        {{#plagiarismlinks}}
                            <span class="sr-only">{{#str}} plagiarism_report, mod_coursework {{/str}}</span>{{{plagiarismlinks}}}<br>
                        {{/plagiarismlinks}}
                    {{/files}}
                {{/submissiondata}}
                {{^submissiondata}}
                    {{^extensiongranted}}
                        {{#duedate}}
                            <div class="duedate-container">
                                <span class="sr-only">{{#str}} due_date, mod_coursework {{/str}}</span>
                                <small>Due:
                                    {{#userdate}}
                                        {{.}},{{#str}}strftimedatetime, langconfig{{/str}}
                                    {{/userdate}}
                                </small>
                            </div>
                        {{/duedate}}
                    {{/extensiongranted}}
                {{/submissiondata}}
            {{/submission}}
        </td>

        {{! Marking. }}
        <td class="marking-col">
            <ol class="list-unstyled p-0 m-0" aria-label="Markers and agreed mark">
                {{#markers}}
                <li class="d-flex mb-2">
                    <div>
                        <span class="sr-only">{{#str}} assessor, mod_coursework {{/str}}</span>
                        <a href="{{{markerurl}}}" class="d-flex align-items-center">
                            {{#markerimg}}
                                <div class="mr-2 bg-cover bg-light rounded-circle flex-shrink-0" style="background-image: url('{{markerimg}}'); height: 2rem; width: 2rem;"></div>
                            {{/markerimg}}
                            {{markername}}
                        </a>
                    </div>

                    {{#mark}}
                    <div class="text-right ml-auto">
                        <span class="sr-only">{{#str}} mark, mod_coursework {{/str}}</span>

                        {{#draft}}
                        <span class="badge badge-secondary">{{#str}} draft, mod_coursework {{/str}}</span>
                        {{/draft}}

                        {{! Finalised. }}
                        {{^draft}}
                            {{#timemodified}}
                                <small>
                                {{#userdate}}
                                    {{.}},{{#str}}strftimedatetime, langconfig{{/str}}
                                {{/userdate}}
                                </small>
                            {{/timemodified}}
                        {{/draft}}

                        <a href="{{{markurl}}}" class="font-weight-bold pl-1">{{.}}</a>
                    </div>
                    {{/mark}}

                    {{#addfeedback}}
                    <div class="text-right ml-auto">
                        <a href="{{{markurl}}}" class="btn btn-primary btn-sm">{{#str}} add_feedback, mod_coursework {{/str}}</a>
                    </div>
                    {{/addfeedback}}
                </li>
                {{/markers}}
                <!-- No allocated markers. -->
                {{^markers}}
                <li class="d-flex mb-2">
                    <div>
                        hello
                    </div>
                    {{#addfeedback}}
                        <div class="text-right ml-auto">
                            <a href="{{{url}}}" class="btn btn-primary btn-sm">{{#str}} add_feedback, mod_coursework {{/str}}</a>
                        </div>
                    {{/addfeedback}}
                </li>
                {{/markers}}

                {{#agreedmark}}
                <li class="d-flex mb-2">
                    <div>Agreed mark</div>
                    <div class="text-right ml-auto">
                    {{#mark}}
                        {{#draft}}
                            <span class="badge badge-secondary">{{#str}} draft, mod_coursework {{/str}}</span>
                        {{/draft}}

                        {{#readyforrelease}}
                            <span class="badge badge-warning">{{#str}} ready_release, mod_coursework {{/str}}</span>
                        {{/readyforrelease}}

                        {{! Released. }}
                        {{#released}}
                            {{#timemodified}}
                                <small>
                                {{#userdate}}
                                    {{.}},{{#str}}strftimedatetime, langconfig{{/str}}
                                {{/userdate}}
                                </small>
                            {{/timemodified}}
                            <span class="badge badge-success">{{#str}} statusreleased, mod_coursework {{/str}}</span>
                        {{/released}}

                        <a href="{{{url}}}" class="font-weight-bold pl-1">{{mark}}</a>
                    {{/mark}}

                    {{#addfinalfeedback}}
                    <a href="{{{url}}}" class="btn btn-sm btn-success">{{#str}} add_agreed_feedback, mod_coursework {{/str}}</a>
                    {{/addfinalfeedback}}
                </div>
                </li>
                {{/agreedmark}}
            </ol>
        </td>

        {{! Actions. }}
        <td class="text-right">
          {{#actions}}
            {{> mod_coursework/submissions/actions }}
          {{/actions}}
        </td>
</tr>
