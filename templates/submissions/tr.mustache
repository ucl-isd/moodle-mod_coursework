<tr data-status="{{status}}" data-marker="{{{markerfilter}}}">
        {{! User. }}
        <td>
            {{#submissiontype}}
                <div class="d-flex align-items-center">
                    {{#user}}
                        {{#user.picture}}
                            <div class="mr-2 bg-cover bg-light rounded-circle flex-shrink-0" style="background-image: url('{{user.picture}}'); height: 2rem; width: 2rem;"></div>
                        {{/user.picture}}
                        {{#user.url}}
                            <a href="{{{.}}}" class="d-flex align-items-center" target="_blank">
                                {{{user.name}}}
                            </a>
                        {{/user.url}}
                        {{^user.url}}
                            {{user.name}}
                        {{/user.url}}
                    {{/user}}
                    {{#group}}
                        <div class="d-inline-block text-left">
                            <div class="d-flex align-items-center">
                                {{#group.picture}}
                                    <div class="mr-2 bg-cover bg-light rounded-circle flex-shrink-0" style="background-image: url('{{group.picture}}'); height: 2rem; width: 2rem;"></div>
                                {{/group.picture}}
                                {{group.name}}
                            </div>
                            <div class="dropdown mt-1">
                                <button class="btn btn-secondary btn-sm dropdown-toggle mt-1" type="button" data-toggle="dropdown" aria-expanded="false">
                                    {{#str}}viewmembers, mod_coursework{{/str}}
                                </button>
                                <ul class="dropdown-menu">
                                {{#members}}
                                    <li><a class="dropdown-item" href="{{url}}" target="_blank">{{name}}</a></li>
                                {{/members}}
                                </ul>
                            </div>
                        </div>
                    {{/group}}
                </div>
            {{/submissiontype}}
        </td>

        {{! Submission. }}
        <td>
            {{#submission}}

                {{! Extension. }}
                <div class="extensiongranted-container {{^extensiondeadline}}d-none{{/extensiondeadline}} small">
                    <span class="badge badge-dark">{{#str}} extension_granted, mod_coursework {{/str}}</span>
                        <span class="extensiongranteddate">
                            {{#extensiondeadline}}
                                {{#userdate}}
                                    {{extensiondeadline}},{{#str}}strftimedatetime, langconfig{{/str}}
                                {{/userdate}}
                            {{/extensiondeadline}}
                        </span>
                </div>

                {{#submissiondata}}
                    {{#datemodified}}
                        <small>
                        {{#submittedlate}}
                            <span class="badge badge-warning">
                            {{#str}} submitted_late, mod_coursework {{/str}}
                            </span>
                        {{/submittedlate}}

                        {{^submittedlate}}
                            {{#str}} submitted, mod_coursework {{/str}}:
                        {{/submittedlate}}
                        {{#userdate}}
                            {{.}},{{#str}}strftimedatetime, langconfig{{/str}}
                        {{/userdate}}
                        <br>
                        </small>
                    {{/datemodified}}

                    {{#files}}
                        <span class="sr-only">
                            {{#str}} file_submitted, mod_coursework {{/str}}</span>
                        <a href="{{{url}}}" class="line-clamp-1" style="max-width:300px;">
                            <i class="fa-regular fa-file-lines mr-2"></i>{{filename}}
                        </a>
                        {{#flaggedplagiarism}}
                        <span class="badge badge-danger">{{#str}} flagged_plagiarism, mod_coursework {{/str}}</span><br>
                        {{/flaggedplagiarism}}

                        <span class="core_plagiarism_links"><div class="tii_links_container"><div class="turnitin_status"><img class="icon icon_size" alt="Turnitin ID: 257478421" title="Turnitin ID: 257478421" src="https://moodle.ucl.ac.uk/theme/image.php/ucl/plagiarism_turnitin/1755762632/turnitin-icon">Turnitin ID: 257478421</div><div class="row_score pp_origreport pp_origreport_open origreport_257478421_7265272" tabindex="0" role="link"><div title="13% Similarity" class="tii_tooltip origreport_score score_colour score_colour_25">13%</div><div class="launch_form origreport_form_257478421"></div></div><div class="student_read_icon"><img class="icon " alt="The student has not viewed this paper." title="The student has not viewed this paper." src="https://moodle.ucl.ac.uk/theme/image.php/ucl/plagiarism_turnitin/1755762632/icon-dot"></div><div class="clear"></div></div><span><!-- Turnitin Plagiarism plugin Version: 2025073101 Course ID: 7703782 TII assignment ID: 21052090 --></span></span>

                        {{#plagiarismlinks}}
                            <span class="sr-only">{{#str}} plagiarism_report, mod_coursework {{/str}}</span>

                            {{{plagiarismlinks}}}

                        {{/plagiarismlinks}}
                    {{/files}}
                {{/submissiondata}}

                {{^submissiondata}}
                    {{^extensiongranted}}
                        {{#duedate}}
                            <div class="duedate-container">
                                <span class="sr-only">{{#str}} due_date, mod_coursework {{/str}}</span>
                                <small>Due:
                                    {{#userdate}}
                                        {{.}},{{#str}}strftimedatetime, langconfig{{/str}}
                                    {{/userdate}}
                                </small>
                            </div>
                        {{/duedate}}
                    {{/extensiongranted}}
                {{/submissiondata}}
            {{/submission}}
        </td>

        {{! Marking. }}
        <td class="marking-col">
            <ol class="list-unstyled p-0 m-0" aria-label="Markers and agreed mark">
                {{#markers}}
                <li class="d-flex mb-2">
                    <div>
                        <span class="sr-only">{{#str}} assessor, mod_coursework {{/str}}</span>
                        <a href="{{{markerurl}}}" class="d-flex align-items-center">
                            {{#markerimg}}
                                <div class="mr-2 bg-cover bg-light rounded-circle flex-shrink-0" style="background-image: url('{{markerimg}}'); height: 2rem; width: 2rem;"></div>
                            {{/markerimg}}
                            {{markername}}
                        </a>
                    </div>

                    {{#mark}}
                    <div class="text-right ml-auto">
                        <span class="sr-only">{{#str}} mark, mod_coursework {{/str}}</span>

                        {{#draft}}
                        <span class="badge badge-secondary">{{#str}} draft, mod_coursework {{/str}}</span>
                        {{/draft}}

                        {{! Finalised. }}
                        {{^draft}}
                            {{#timemodified}}
                                <small>
                                {{#userdate}}
                                    {{.}},{{#str}}strftimedatetime, langconfig{{/str}}
                                {{/userdate}}
                                </small>
                            {{/timemodified}}
                        {{/draft}}

                        <a id="edit-feedback-{{allocatablehash}}" href="{{{markurl}}}" class="font-weight-bold pl-1">{{.}}</a>
                    </div>
                    {{/mark}}

                    {{#addfeedback}}
                    <div class="text-right ml-auto">
                        <a id="add-feedback-{{allocatablehash}}" href="{{{markurl}}}" class="btn btn-primary btn-sm">{{#str}} add_feedback, mod_coursework {{/str}}</a>
                    </div>
                    {{/addfeedback}}
                </li>
                {{/markers}}
                <!-- No allocated markers. -->
                {{^markers}}
                <li class="d-flex mb-2">
                    <div>
                        hello
                    </div>
                    {{#addfeedback}}
                        <div class="text-right ml-auto">
                            <a id="add-feedback-{{allocatablehash}}" href="{{{url}}}" class="btn btn-primary btn-sm">{{#str}} add_feedback, mod_coursework {{/str}}</a>
                        </div>
                    {{/addfeedback}}
                </li>
                {{/markers}}

                {{#agreedmark}}
                <li class="d-flex mb-2">
                    <div>Agreed mark</div>
                    <div class="text-right ml-auto">
                    {{#mark}}
                        {{#draft}}
                            <span class="badge badge-secondary">{{#str}} draft, mod_coursework {{/str}}</span>
                        {{/draft}}

                        {{#readyforrelease}}
                            <span class="badge badge-warning">{{#str}} ready_release, mod_coursework {{/str}}</span>
                        {{/readyforrelease}}

                        {{! Released. }}
                        {{#released}}
                            {{#timemodified}}
                                <small>
                                {{#userdate}}
                                    {{.}},{{#str}}strftimedatetime, langconfig{{/str}}
                                {{/userdate}}
                                </small>
                            {{/timemodified}}
                            <span class="badge badge-success">{{#str}} statusreleased, mod_coursework {{/str}}</span>
                        {{/released}}

                        <a id="edit-agreed-feedback-{{allocatablehash}}" href="{{{url}}}" class="font-weight-bold pl-1">{{mark}}</a>
                    {{/mark}}

                    {{#addfinalfeedback}}
                    <a id="add-agreed-feedback-{{allocatablehash}}" href="{{{url}}}" class="btn btn-sm btn-success">{{#str}} add_agreed_feedback, mod_coursework {{/str}}</a>
                    {{/addfinalfeedback}}
                </div>
                </li>
                {{/agreedmark}}
            </ol>
        </td>

        {{! Actions. }}
        <td class="text-right">
          {{#actions}}
            {{> mod_coursework/submissions/actions }}
          {{/actions}}
        </td>
</tr>
